<?xml version="1.0" encoding="utf-8"?>
<mapStylePresets>
	<!-- type="general|pedestrian|bicycle|car|custom" defines how preset is treated. -->
	<!-- general, pedestrian, bicycle, car have special names and icons, while for custom
	"name" attribute is used along with common icon -->

	<!-- Presets for "default.render.xml" map style. -->
	<!-- If name of style changes (e.g. "default.render.xml"
	will change to "my_default.render.xml"), then
	change style attribute here. -->
	<mapStylePreset style="default" type="general">
		<attribute name="appMode" value="browse map" />
	</mapStylePreset>
	<mapStylePreset style="default" type="pedestrian">
		<attribute name="appMode" value="pedestrian" />
	</mapStylePreset>
	<mapStylePreset style="default" type="bicycle">
		<attribute name="appMode" value="bicycle" />
	</mapStylePreset>
	<mapStylePreset style="default" type="car">
		<attribute name="appMode" value="car" />
	</mapStylePreset>

	<!-- Custom preset samples for some "third_party_style.render.xml" -->
	<!-- type="custom" attribute may be omitted, since it's the default value -->
	<!--
	<mapStylePreset style="third_party_style" type="custom" name="no polygons and no boundaries">
		<attribute name="noAdminboundaries" value="true" />
		<attribute name="noPolygons" value="true" />
	</mapStylePreset>
	-->
</mapStylePresets>
